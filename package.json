{
   "name": "vscode-malinajs-highlight",
   "displayName": "Malina Syntax Highlighter",
   "description": "Syntax highlightning for Malina.js framework",
   "version": "0.1.1",
   "publisher": "Ulmzr",
   "engines": {
      "vscode": "^1.68.0"
   },
   "categories": [
      "Programming Languages"
   ],
   "contributes": {
      "languages": [
         {
            "id": "xht",
            "aliases": [
               "Malina",
               "xht"
            ],
            "extensions": [
               ".xht",
               ".ma"
            ],
            "configuration": "./language-configuration.json"
         }
      ],
      "grammars": [
         {
            "language": "xht",
            "scopeName": "source.xht",
            "path": "./syntaxes/xht.tmLanguage.json"
         }
      ]
   }
}