{
   "name": "malinajs-highlighter",
   "displayName": "MalinaJS Syntax Highlighter",
   "description": "Syntax highlightning for MalinaJS framework",
   "version": "0.0.3",
   "engines": {
      "vscode": "^1.47.0"
   },
   "icon": "icons/logo.png",
   "galleryBanner": {
      "color": "#333333",
      "theme": "dark"
   },
   "categories": [
      "Programming Languages"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/ulmzr/malinajs-highlighter-vscode.git"
   },
   "homepage": "https://github.com/ulmzr/malinajs-highlighter-vscode#readme",
   "publisher": "ulmzr",
   "contributes": {
      "languages": [
         {
            "id": "malinajs",
            "aliases": [
               "Malina.js",
               "malinajs"
            ],
            "extensions": [
               ".xht",
               ".ma"
            ],
            "configuration": "./language-configuration.json"
         }
      ],
      "grammars": [
         {
            "language": "malinajs",
            "scopeName": "source.malinajs",
            "path": "./syntaxes/malinajs.tmLanguage.json",
            "embeddedLanguages": {
               "text.html": "xml",
               "source.css": "css",
               "source.js": "javascript"
            }
         }
      ]
   }
}
